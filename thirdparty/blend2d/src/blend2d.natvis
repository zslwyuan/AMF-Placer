        <ValuePointer Condition="implType == BL_IMPL_TYPE_ARRAY_I8">(const int8_t*)data</ValuePointer>
        <ValuePointer Condition="implType == BL_IMPL_TYPE_ARRAY_U8">(const uint8_t*)data</ValuePointer>
